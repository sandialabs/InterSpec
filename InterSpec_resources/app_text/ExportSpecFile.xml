<?xml version="1.0" encoding="UTF-8"?>
<messages nplurals="2" plural="n==1 ? 0 : 1">
  <!-- esf- is short for ExportSpecFile -->
  
  <!-- Window title -->
  <message id="esf-window-title">Spectrum File Export</message>
  
  <!-- Main UI sections -->
  <message id="esf-file-to-export">File to Export</message>
  <message id="esf-file-format">File Format</message>
  <message id="esf-samples-to-include">Samples to Include</message>
  
  <!-- Mobile tab titles -->
  <message id="esf-file-tab">File</message>
  <message id="esf-format-tab">Format</message>
  <message id="esf-options-tab">Options</message>
  
  <!-- Checkbox labels for foreground/background combination -->
  <message id="esf-plus-back"> + Back.</message>
  <message id="esf-plus-secondary"> + Secon.</message>
  <message id="esf-fore-plus-back-tt">Export the foreground and background and/or secondary spectra together into the same file.</message>
  
  <!-- Sample selection checkboxes -->
  <message id="esf-disp-foreground">Disp. Foreground</message>
  <message id="esf-disp-background">Disp. Background</message>
  <message id="esf-disp-secondary">Disp. Secondary</message>
  <message id="esf-all-samples">All Samples</message>
  <message id="esf-custom-samples">Custom Samples</message>
  <message id="esf-custom-samples-tt">Enter the sample number you'de like displayed here. You may enter a range of sample numbers similar to '33-39' or '33 to 39', or CSV sample numbers like '34,39,84'</message>
  <message id="esf-filter-detectors">Filter Detectors</message>
  
  <!-- Options section -->
  <message id="esf-options">Options</message>
  <message id="esf-sum-to-single-record">Sum to single record</message>
  <message id="esf-sum-to-single-record-tt">All selected samples and/or detectors will be summed together into a single spectrum.<br />Caution: if you have both foreground and background selected, they will be summed together.</message>
  
  <message id="esf-sum-fore-to-single-record">Sum Fore to single record</message>
  <message id="esf-sum-fore-to-single-record-tt">There are either multiple sample numbers, or multiple detectors that comprise the displayed foreground; checking this option will sum all these to a single spectrum in the output file.</message>
  
  <message id="esf-sum-back-to-single-record">Sum Back to single record</message>
  <message id="esf-sum-back-to-single-record-tt">There are either multiple sample numbers, or multiple detectors that comprise the displayed background; checking this option will sum all these to a single spectrum in the output file.</message>
  
  <message id="esf-sum-sec-to-single-record">Sum Sec to single record</message>
  
  <message id="esf-background-subtract">Background subtract</message>
  <message id="esf-background-subtract-tt">The output spectrum will be the foreground, minus the background.</message>
  
  <message id="esf-sum-det-per-sample">Sum det. per sample</message>
  <message id="esf-sum-det-per-sample-tt">For each sample number, sum all detectors for that sample number together.<br />E.g., if you have 8 detectors in your spectrum file, each with 120 samples then the results will have 120 spectra, each of which is the sum of the 8 detectors of the respective samples.</message>
  
  <message id="esf-sum-samples-per-det">Sum samples per det.</message>
  <message id="esf-sum-samples-per-det-tt">For each detector, sums all sample numbers together.<br />E.g., if you have 8 detectors in your spectrum file, each with 120 samples then the results will have 8 spectra, each of which is the sum of the 120 samples of their respective detectors.</message>
  
  <message id="esf-remove-interspec-info">Remove InterSpec info</message>
  <message id="esf-remove-interspec-info-tt">Removes detector response, peaks, and other analysis results you may have added in InterSpec.</message>
  
  <message id="esf-remove-gps">Remove GPS</message>
  
  <!-- Status text -->
  <message id="esf-not-applicable">Not Applicable</message>
  <message id="esf-none-available">None Available</message>
  
  <!-- Buttons -->
  <message id="esf-export">Export</message>
  <message id="esf-show-qr-code">Show QR-code</message>
  
  <!-- File info table headers -->
  <message id="esf-displayed-as">Displayed As</message>
  <message id="esf-fore-abbrev">For.</message>
  <message id="esf-back-abbrev">Back</message>
  <message id="esf-sec-abbrev">Sec</message>
  
  <!-- QR Code dialog -->
  <message id="esf-next-qr-code">Next QR code</message>
  <message id="esf-url-link">URL Link</message>
  <message id="esf-email-link">Email Link</message>
  
  <!-- Error dialogs -->
  <message id="esf-error-title">Error</message>
  <message id="esf-qr-encode-failed">Failed to display spectrum as a QR code: {1}</message>
  
  <!-- Pluralized messages -->
  <message id="esf-disp-sample">
    <plural case="0">Disp.&nbsp;Sample</plural>
    <plural case="1">Disp.&nbsp;Samples</plural>
  </message>
  
  <!-- Table headers -->
  <message id="esf-start-time">Start Time</message>
  <message id="esf-end-time">End Time</message>
  <message id="esf-first-sample">First Sample</message>
  <message id="esf-last-sample">Last Sample</message>
  <message id="esf-num-samples">Num. Samples</message>
  <message id="esf-neut-dets">Neut. Dets</message>
  <message id="esf-gamma-dets">Gamma Dets</message>
  <message id="esf-num-dets">Num Dets</message>
  <message id="esf-latitude">Latitude</message>
  <message id="esf-longitude">Longitude</message>
  <message id="esf-total-time">Total Time</message>
  <message id="esf-sum-gamma">Sum Gamma</message>
  <message id="esf-sum-neut">Sum Neut.</message>
  <message id="esf-manufacturer">Manufacturer</message>
  <message id="esf-model">Model</message>
  <message id="esf-serial">Serial</message>
</messages>