  <message id="ctwidget-medical">चिकित्सा</message>
  <message id="ctwidget-industrial">औद्योगिक</message>
  <message id="ctwidget-norm">NORM</message>
  <message id="ctwidget-snm">SNM</message>
  <message id="ctwidget-common">सामान्य</message>
  <message id="ctwidget-other">अन्य</message>

  <!-- Other options -->
  <message id="ctwidget-other-options">अन्य विकल्प</message>
  <message id="ctwidget-peak-label-size">पीक लेबल आकार</message>
  <message id="ctwidget-peak-label-size-desc">पीक लेबल का फ़ॉन्ट-आकार निर्दिष्ट करता है</message>
  <message id="ctwidget-peak-label-angle">पीक लेबल कोण</message>
  <message id="ctwidget-peak-label-angle-desc">पीक लेबल का घुमाव, डिग्री में</message>
  <message id="ctwidget-log-y-min">Log-Y न्यूनतम</message>
  <message id="ctwidget-log-y-min-desc">लघुगणकीय y-अक्ष पर प्रदर्शित न्यूनतम मान, जब प्रदर्शित x-सीमा में शून्य या नकारात्मक गणना वाला चैनल हो।</message>
